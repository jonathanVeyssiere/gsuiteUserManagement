function STEP2_PACMAN_SUSPENDED_EXTRACT(){  
  
  //Name of the sheet and tab
  var document         = SpreadsheetApp.getActive();
  var mysheetlist      = document.getSheetByName("DELETION PROCESSING LIST");
  
  //Delete content on sheet
  mysheetlist.getRange("C2:C").clear();
  
  //Name of the sheet and tab  
  var document       = SpreadsheetApp.getActive();
  var sheet          = document.getSheetByName("ACCOUNTS TO STUDY");
  
  //Delete content on sheet
  sheet.getRange("A2:B").clear();
  
  var pageToken;
  var page, dataToInsert = [];
  do 
     {
    page = AdminDirectory.Users.list
         (
         {
          //Primary Domain Name 
          domain: 'decathlon.com',
          //Ordered by ...
          orderBy: 'givenName',
          pageToken: pageToken
         }
         );
         var users     = page.users;
           if (users) 
            {
            //Boucle d'Ã©criture de la liste
            for (var i = 0; i < users.length; i++) 
                {
                 var user = users[i];
                 //add on sheet if user is suspended
                  if (user.suspended)
                   {
                     //process completely before writing on sheets aiming at optimization regarding timed out script   
                     dataToInsert.push([user.primaryEmail,user.orgUnitPath]);
                   } 
                }
            } 
           else 
            {
             Browser.msgBox("Error", "Pas d'utilisateur dans le domaine !", Browser.Buttons.OK);
             return;
             }
             pageToken = page.nextPageToken;
     } 
     while (pageToken);

  // Set the results.
  sheet.getRange(2, 1, dataToInsert.length, 2).setValues(dataToInsert);
}
