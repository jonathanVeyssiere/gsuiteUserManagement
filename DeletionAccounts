function STEP4_PACMAN_JOB(){  
    
  //Préciser dans la feuille active le nom de la feuille et des onglets 
  var document         = SpreadsheetApp.getActive();
  var mysheetlist      = document.getSheetByName("DELETION PROCESSING LIST"); 
    
  //Supprimer des contenus dans le fichier
  mysheetlist.getRange("C3:C").clearContent();  
  
  //Définition de la liste des comptes a supprimer
  var myContact      = mysheetlist.getDataRange().getValues();

  //Boucle d'extraction des comptes
  for (i=1; i < myContact.length; i++) 
  {var personPositif = myContact[i];
    
    //Condition de suppression des comptes
    if (personPositif[0] != "" && (personPositif[0]=="andrea.latini@decathlon.com" || personPositif[0]=="allan.cadet@decathlon.com")) 
    {
       
      //Extraire les mobiles registered des users
      var responseapi      = AdminReports.Activities.list(personPositif[0], 'mobile',{ eventName: "DEVICE_REGISTER_UNREGISTER_EVENT",  maxResults: 1000 });
      
      //Logger : mobiles registered des users
      for (i=0; i < responseapi['items'].length; i++) 
      {
        //Suppression des comptes 
        Logger.log(personPositif[0])
        Logger.log(responseapi['items'][i]['events'][0]['parameters'][5]['value']);
        var actionapi      = AdminDirectory.Mobiledevices.action(
          personPositif[0], 
          responseapi['items'][i]['events'][0]['parameters'][5]['value'], 
          { action: "admin_account_wipe" });
      }
      
      //AdminDirectory.Users.remove(personPositif[0]);
      //Fonction de marque de succès de la tâche de suppression
      mysheetlist.getRange(i+1,3).setValue("Yes");
    }
  }
  //Email archivage
  GmailApp.sendEmail("collaborative.tools@decathlon.com", "Suppression de comptes SUSPENDED effectué",personPositif[0]);
  
  //Message Box de succès
  Browser.msgBox("DELETION COMPLETED", "Tous les comptes ont bien été supprimé.", Browser.Buttons.OK);
  return; 
}
